<?xml version="1.0" encoding="utf-8"?>
<base>
  <Current_Tuning>
    <kCameraTuning>
      <AdditionalCostToTakePhoto value="0">
      </AdditionalCostToTakePhoto>
      <MaximumSize value="Medium">
        <!--Maximum available photo size.  May be gated by skill.-->
      </MaximumSize>
      <MaximumZoom value="Maximum">
        <!--Maximum available zoom without ZoomUpgrade.-->
      </MaximumZoom>
      <MaximumZoomWithZoomUpgrade value="Maximum">
        <!--Maximum available zoom with ZoomUpgrade.-->
      </MaximumZoomWithZoomUpgrade>
      <SkillGainMultiplier value="0">
        <!--If non-zero, photos taken will contribute to skill and be sellable, a zero here makes a camera work like the base game cellphone camera.-->
      </SkillGainMultiplier>
      <ParisMarketDiscountFactor value="0.2">
        <!--Proportion of normal price the camera should cost at the market stall (vs. a normal store).-->
      </ParisMarketDiscountFactor>
      <ShortcutPriority value="1337">
        <!--Priority for chosing this camera when player uses the Shift-C shortcut.  Ideally each camera should have a distinct value.-->
      </ShortcutPriority>
      <ValueMultiplier value="1">
        <!--Photo value multiplier.-->
      </ValueMultiplier>
      <CameraQuality value="Expensive">
        <!--Quality level of this camera.-->
      </CameraQuality>
      <MinimumSize value="Medium">
        <!--The minimum PhotographSize that this camera is capable of taking pictures using-->
      </MinimumSize>
    </kCameraTuning>
  </Current_Tuning>
</base>