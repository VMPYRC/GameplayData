      <!--Range:  0 - 100  Description:  Number of points to add for the Athletic skill if the sim catches the object-->
    </kSkillPointsAthletic>
    <kSkillPointsLogic value="100">
      <!--Range:  0 - 100  Description:  Number of points to add for the Logic skill if the sim flinches-->
    </kSkillPointsLogic>
    <kSkillPointsScience value="0">
      <!--Range:  0 - 100  Description:  Number of points to add for the science skill on the actor-->
    </kSkillPointsScience>
  </Current_Tuning>
</base>