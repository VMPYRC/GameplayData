<?xml version="1.0" encoding="utf-8"?>
<base>
  <Current_Tuning>
    <kMotionRunTime value="4">
      <!--Range:  0 - infinity  Description:  This tunable sets the amount of time that a motion is going to be running before it pauses for a delay-->
    </kMotionRunTime>
    <kDelayArray value="0, 3, 5, 8">
      <!--Range:  0 - 100  Description:  This tunable represents the delay possibilities for the set delay interaction-->
    </kDelayArray>
    <kColorRunTime value="5">
      <!--Range:  0 - infinity  Description:  This tunable sets the amount of time that a color is going to be before changing if its in randomizing mode-->
    </kColorRunTime>
    <kLightIntensity value="0.3">
      <!--Float: 0 - infinity  Description:  This tunable sets the intensity of the light when turned on. Adjust if too bright-->
    </kLightIntensity>
  </Current_Tuning>
</base>