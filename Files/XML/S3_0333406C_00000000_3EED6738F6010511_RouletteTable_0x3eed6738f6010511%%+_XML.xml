      <!--Odds increase/decrease for hacked table-->
    </kOddsChangeForHackedTable>
    <kMoodPartyScoreModifier value="2">
      <!--Party score change when someone wins a hand-->
    </kMoodPartyScoreModifier>
    <kSocialGain value="50">
      <!--Social motive increase when used with another sim-->
    </kSocialGain>
    <kChanceOfShock value="20">
      <!--Odds of getting electrocuted while hacking-->
    </kChanceOfShock>
    <kChanceOfFailedSabotage value="30">
      <!--Percent of time sabotage table fails-->
    </kChanceOfFailedSabotage>
    <kOddsChangeForLuckyCoin value="20">
      <!--Odds increase when holding a lucky coin-->
    </kOddsChangeForLuckyCoin>
