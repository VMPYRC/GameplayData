<?xml version="1.0" encoding="utf-8"?>
<base>
  <Current_Tuning>
    <kLowStakes value="100">
      <!--Range: Positive int. Buy-in amount when playing low stakes.-->
    </kLowStakes>
    <kMediumStakes value="500">
      <!--Range: Positive int. Buy-in amount when playing medium stakes.-->
    </kMediumStakes>
    <kHighStakes value="1000">
      <!--Range: Positive int. Buy-in amount when playing high stakes.-->
    </kHighStakes>
    <kMegaStakes value="10000">
      <!--Range: Positive int. Buy-in amount when playing high stakes.-->
    </kMegaStakes>
    <kWinningOdds value="30, 33, 36, 40, 45, 50, 55, 60, 65, 70, 75">
      <!--Roulette base winning odds based on skill level.  Will be normalized and randomized, so they are approximate-->
