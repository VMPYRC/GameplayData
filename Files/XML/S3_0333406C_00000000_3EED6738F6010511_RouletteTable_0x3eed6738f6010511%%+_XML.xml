    <kBetMax value="1.5">
      <!--Multiplier for big bets-->
    </kBetMax>
    <kBetMin value="0.8">
      <!--Multiplier for small bets-->
    </kBetMin>
    <kGamblingGainPerSimMinute value="7">
      <!--Rate gambling skill increases every sim minute.  GPE Default: 30-->
    </kGamblingGainPerSimMinute>
    <kMinutesToPlay value="360">
      <!--Minutes to play before being done, barring stress exits-->
    </kMinutesToPlay>
    <kChanceOfGoodHack value="75">
      <!--Percent of time hacking table results in better results-->
    </kChanceOfGoodHack>
    <kHackDuration value="720">
      <!--Duration of hack in sim minutes.  GPE Default 1440 = 1 sim day-->
    </kHackDuration>
    <kWatchRadius value="2">
