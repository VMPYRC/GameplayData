    <kMinScienceSkillLevel value="0">
      <!--Range:  0 - 10  Description:  The level required in the Science Skill in order to use this interaction-->
    </kMinScienceSkillLevel>
    <kScienceSkillGain value="0">
      <!--Description:  The skill gain the sim will receive to the science skill upon finishing this interaction-->
    </kScienceSkillGain>
    <kExperimentLoopDuration value="15">
      <!--Description:  The duration of the looping portion of the interaction in sim minutes-->
    </kExperimentLoopDuration>
    <kSuccessChance value="0.06">
      <!--Range:  0 - 1  Description:  This is the chance that the interaction will succeed (Higher value means higher success rate).-->
