<?xml version="1.0" encoding="utf-8"?>
<base>
  <Current_Tuning>
    <kEnvironmentTuning>
      <EnvironmentScore value="0">
        <!--Range: Any float value.  Description:  Each game object can define an environment score to contribute to the room environment score when Sims are in it.  It is expected that fancier, pretty objects will give a positive Environment score while dirty, ugly objects will give a negative Environment score.  The score contribution is defined by this tunable.-->
      </EnvironmentScore>
    </kEnvironmentTuning>
    <kViewTuning>
      <MinDistanceToView value="2">
        <!--Minimum Distance at which the Sim will watch the painting. Positive float.-->
      </MinDistanceToView>
      <MaxDistanceToView value="3">
        <!--Maximum Distance at which the Sim will watch the painting. Positive float.-->
      </MaxDistanceToView>
    </kViewTuning>
    <kMaxStackIndoors value="2">
      <!--Positive int - Maximum number of pedestals that can be stacked indoors.-->
    </kMaxStackIndoors>
    <kMaxStackOutdoors value="10">
      <!--Positive int - Maximum number of pedestals that can be stacked outdoors.-->
    </kMaxStackOutdoors>
    <kAllowStackingOnNonPedestals value="True">
      <!--Boolean - If false, do not allow pedestal to be stacked on top of a non-pedestal object-->
    </kAllowStackingOnNonPedestals>
  </Current_Tuning>
</base>