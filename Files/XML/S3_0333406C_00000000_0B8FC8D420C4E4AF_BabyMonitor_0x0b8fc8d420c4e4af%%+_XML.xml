    <kPeekabooSimMinutesBetweenLTRUpdates value="5">
      <!--Peekaboo. Number of sim minutes between LTR updates between the two sims.-->
    </kPeekabooSimMinutesBetweenLTRUpdates>
    <kPeekabooLTRLikingDelta value="10">
      <!--Peekaboo.  The amount of LTR liking increase between the two sims each time that it increases.-->
    </kPeekabooLTRLikingDelta>
    <kAutonomousCooldownTime value="50">
      <!--Time before autonomy can trigger it again, in sim minutes-->
    </kAutonomousCooldownTime>
    <kFakeBabyAutonomyRefresh value="10">
      <!--Time to do fake autonomy checks for baby, in sim minutes-->
    </kFakeBabyAutonomyRefresh>
    <kPeekabooDuration value="60">
      <!--Duration of peekaboo game in minutes.-->
    </kPeekabooDuration>
