    <kRoutingDistance value="1">
      <!--Proximity that a baby will route toward-->
    </kRoutingDistance>
    <kAnswerCallRelationLTRChange value="5">
      <!--Increase to LTR when answering a baby monitor call-->
    </kAnswerCallRelationLTRChange>
    <kPeekabooChanceToPlayInsteadOfGrownupInteraction value="70">
      <!--Peekaboo.  Chance that two toddlers in the same room will play peekabo instead of calling an adult.-->
