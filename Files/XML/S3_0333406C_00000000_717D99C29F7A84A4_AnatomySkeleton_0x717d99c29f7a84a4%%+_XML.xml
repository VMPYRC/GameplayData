    </kIncreasedChanceForRomantic>
    <kBalloonNames value="balloon_microscope, balloon_bodysculptor, balloon_heart, balloon_meat, balloon_bandaid, balloon_caduceus, balloon_medbottle, balloon_pill, balloon_pumped, balloon_jogger, balloon_stethoscope, ep9_balloon_bone_up_on_anatomy_bones, ep9_balloon_bone_up_on_anatomy_eye_ball, ep9_balloon_bone_up_on_anatomy_hands, ep9_balloon_bone_up_on_anatomy_heart, ep9_balloon_bone_up_on_anatomy_medicine_bottle, ep9_balloon_bone_up_on_anatomy_needle_shot, ep9_balloon_bone_up_on_anatomy_pills, ep9_balloon_bone_up_on_anatomy_pumped_sim, ep9_balloon_bone_up_on_anatomy_stethoscope, ep9_balloon_bone_up_on_anatomy_strong_arm">
