    <kMinZombieRadius value="0.75">
      <!--Range: 0 - x  Min radius from object that Zombie sims will go to-->
    </kMinZombieRadius>
    <kMaxZombieRadius value="1">
      <!--Range: 0 - x  Max radius from object that Zombie sims will go to-->
    </kMaxZombieRadius>
    <kMinFairyRadius value="0.5">
      <!--Range: 0 - x  Min radius from object that Fairy sims will go to-->
    </kMinFairyRadius>
    <kMaxFairyRadius value="1">
      <!--Range: 0 - x  Max radius from object that Fairy sims will go to-->
    </kMaxFairyRadius>
    <kMaxZombieLookTime value="30">
      <!--Range: 0 - x  Max time in Sim mins for Zombie look at to last-->
    </kMaxZombieLookTime>
    <kMaxFairyLookTime value="20">
      <!--Range: 0 - x  Max time in Sim mins for Fairy look at to last-->
    </kMaxFairyLookTime>
    <kChanceOfElectrocution value="65">
      <!--Range: 0 - 100  Chance of fairy getting electrocuted-->
    </kChanceOfElectrocution>
  </Current_Tuning>
</base>