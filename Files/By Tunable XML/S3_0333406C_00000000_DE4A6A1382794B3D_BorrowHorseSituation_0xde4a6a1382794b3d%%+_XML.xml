    <kHorseReturnLTRHitInterval value="60">
      <!--Interval in sim minutes where a sim will lose LTR with the horse's household's family members.-->
    </kHorseReturnLTRHitInterval>
    <kHorseReturnLTRDelta value="-5">
      <!--LTR Delta per interval that the rider will receive when the horse is not returned.-->
    </kHorseReturnLTRDelta>
  </Current_Tuning>
</base>