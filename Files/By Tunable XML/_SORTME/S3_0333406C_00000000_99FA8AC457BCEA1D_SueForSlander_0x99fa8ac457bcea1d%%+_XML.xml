      <!--List of traits that decrease the chances of winning a case.-->
    </kNegativeTraits>
    <kMinSettlementPayout value="200, 400, 1000, 1600, 4000, 6000">
      <!--The min settlement payout per celebrity level if the sim wins the case.-->
