<?xml version="1.0" encoding="utf-8"?>
<base>
  <Current_Tuning>
    <kWaitTimeBeforeUpdatingBreakage value="10, 10">
      <!--Range in sim minutes before breakage is updated for the object.  Randomly selected between these two values.-->
    </kWaitTimeBeforeUpdatingBreakage>
    <kTimeBetweenUpdatingBreakage value="15, 15">
      <!--Range in sim minutes between updating breakage.  Randomly selected after each breakage update.-->
    </kTimeBetweenUpdatingBreakage>
    <kWhackedOutScratchTimeMinMax value="5, 10">
      <!--[Min,Max] Num Sim Mins cat with 'whacked out' buff should do this interaction instead of normal amount of time. Actual will be randomly picked between the 2-->
    </kWhackedOutScratchTimeMinMax>
    <kScratchTimeMinMax value="45, 60">
      <!--[Min,Max] Num Sim Mins Normal amount of time a cat will scratch this object for. Actual will be randomly picked between the 2-->
    </kScratchTimeMinMax>
    <kLargeDogScratchMinRoutingDistance value="0.85">
      <!--Range: Positive Float. Description: Min distance away from scratched object that adult/elder large dogs will route to.-->
    </kLargeDogScratchMinRoutingDistance>
    <kLittleDogScratchMinRoutingDistance value="0.55">
      <!--Range: Positive Floats. Description: Min distance away from scratched object that adult/elder little dogs will route to.-->
    </kLittleDogScratchMinRoutingDistance>
    <kPuppyScratchMinRoutingDistance value="0.2">
      <!--Range: Positive Floats. Description: Min distance away from scratched object that puppies will route to.-->
    </kPuppyScratchMinRoutingDistance>
    <kCatScratchMinRoutingDistance value="0.2">
      <!--Range: Positive Floats. Description: Min distance away from scratched object that adult/elder cats will route to.-->
    </kCatScratchMinRoutingDistance>
    <kKittenScratchMinRoutingDistance value="0.1">
      <!--Range: Positive Floats. Description: Min distance away from scratched object that kittens will route to.-->
    </kKittenScratchMinRoutingDistance>
  </Current_Tuning>
</base>
