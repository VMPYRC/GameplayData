<?xml version="1.0" encoding="utf-8"?>
<base>
  <Current_Tuning>
    <kTotalInvestigateTimeMinMax value="90,240">
      <!--Minimum and maximum time in sim minutes it takes to investigate a drill hole.-->
    </kTotalInvestigateTimeMinMax>
    <kMidAdventureTNSTimeMinMax value="30,90">
      <!--Minimum and maximum time in sim minutes between mid adventure TNSes appearing.-->
    </kMidAdventureTNSTimeMinMax>
    <kMidAdventureNumberOfTNSes value="20">
      <!--Number of mid adventure TNSes.-->
    </kMidAdventureNumberOfTNSes>
    <kFinalOutcomeBuffsGood value="AwesomeParty,Buzzed,Delighted,Fascinated,GreatAdventure,IAmTheGreatest,Inspired,OddlyPowerful,TheLifeOfTheParty,Winner">
