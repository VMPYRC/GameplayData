    </kFinalOutcomeWeightTableGood>
    <kFinalOutcomeWeightTableOK value="105,45,40,15,5,25,25,10,25,5,45,2">
      <!--Okay outcome weighting table.  Order is: {nothing, buff, lp gem, mp gem, hp gem, metal, rock, seed, fish, random gnome, singed skin, caveman skin}-->
    </kFinalOutcomeWeightTableOK>
    <kFinalOutcomeWeightTableBad value="80,65,1,0,0,1,1,0,10,0,60,0">
      <!--Bad outcome weighting table.  Order is: {nothing, buff, lp gem, mp gem, hp gem, metal, rock, seed, fish, random gnome, singed skin, caveman skin}-->
    </kFinalOutcomeWeightTableBad>
    <kFinalOutcomeNumberOfTNSes value="5, 5, 5">
      <!--Number of final outcome TNS strings for Good, OK, and Bad outcomes {good, ok, bad}-->
    </kFinalOutcomeNumberOfTNSes>
    <kFinalOutcomeMaximumRolls value="7, 4, 2">
      <!--Maximum number of rolls for Good, OK, and Bad outcome rewards {good, ok, bad}-->
    </kFinalOutcomeMaximumRolls>
    <kFinalOutcomeMinimumRolls value="3, 2, 1">
      <!--Minimum number of rolls for Good, OK, and Bad outcome rewards {good, ok, bad}-->
    </kFinalOutcomeMinimumRolls>
    <kFinalOutcomeWeightRollWeight value="0.41,0.22,0.22,0.15">
      <!--Outcome weighting {random weight, mood, athletic skill, logic skill}.  Note that these values should add up to 1.0f-->
    </kFinalOutcomeWeightRollWeight>
    <kFinalOutcomeThresholds value="0.4,0.7">
      <!--OK and Good outcome thresholds.  Note that these values should be between 0.0f and 1.0f-->
    </kFinalOutcomeThresholds>
    <kSeedRarityWeights value="10,8,6,2">
      <!--Seed weights for grabbing a seed. {Common, Uncommon, Rare, Special}-->
    </kSeedRarityWeights>
    <kSeedQualityRange value="Bad, Outstanding">
      <!--Range of quality for selecting a seed.-->
    </kSeedQualityRange>
    <kFishMinimumLevel value="3">
      <!--Will choose a random fish above this level.-->
    </kFishMinimumLevel>
    <kMetalList value="Silver,Gold,Palladium,Plutonium,Mercury,Platinum,Copper,Mummitomium,Titanium,Iridium">
