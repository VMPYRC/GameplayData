    </kFinalOutcomeBuffsGood>
    <kFinalOutcomeBuffsOK value="Warmed,Tranquil,Pumped,LetOffSteam,Impressed,Intrigued,Comforted">
      <!--List of buffs to randomly choose from if outcome was okay.-->
    </kFinalOutcomeBuffsOK>
    <kFinalOutcomeBuffsBad value="Backache,Disgusted,Embarrassed,Horrified,Singed,Sore">
      <!--List of buffs to randomly choose from if outcome was bad.-->
    </kFinalOutcomeBuffsBad>
