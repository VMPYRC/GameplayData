    <kMetalProductVersionList value="BaseGame,BaseGame,BaseGame,BaseGame,EP1,EP1,EP1,EP1,EP1,EP1,EP1">
      <!--List of expansion packs to pull from.  Note that there has to be some entries from base game.-->
    </kMetalProductVersionList>
    <kMetalWeights value="60,40,30,5,20,20,30,5,5,5">
      <!--Corresponding weights to how often a metal gets selected.-->
    </kMetalWeights>
