<?xml version="1.0" encoding="utf-8"?>
<base>
  <Current_Tuning>
    <kBaseTauntChance value="0.5">
      <!--Float 0-1. Base chance for kicker to taunt.-->
    </kBaseTauntChance>
    <kTraitBonusTauntChance value="0.3">
      <!--Float 0-1. Sims with a trait in kBonusTauntChanceTraits will gain this bonus chance to taunting.-->
    </kTraitBonusTauntChance>
    <kBonusTauntChanceTraits value="Childish, GoodSenseOfHumor, Evil, MeanSpirited">
      <!--Array of TraitNames. Sims with these traits will have kTraitBonusTauntChance added to the base taunt chances.-->
    </kBonusTauntChanceTraits>
    <kNoTauntingTraits value="Grumpy, NoSenseOfHumor">
      <!--Array of TraitNames. Sims with these traits will not taunt.-->
    </kNoTauntingTraits>
    <kGreaterSkillSuccessChances value="0.5, 0.64, 0.73, 0.79, 0.84, 0.87, 0.91, 0.93, 0.96, 0.98, 1">
      <!--Array of Floats. Chances of Sim with greater skill of succeeding (scoring for kicker, catching for goalie). First value is for skill difference of 1, second value is for skill difference of 2, third is skill difference 3, etc.-->
    </kGreaterSkillSuccessChances>
    <kNumKicks value="4">
      <!--Int 1+. Number of kicks of a kicker gets.-->
    </kNumKicks>
    <kNumMinutesBetweenRelGain value="10">
      <!--Float. How often rel gain happens.-->
    </kNumMinutesBetweenRelGain>
    <kRelGain value="1">
      <!--Float. How much rel gain sims get every kNumMinutesBetweenRelGain.-->
    </kRelGain>
    <kNumFestivalTicketsForWinning value="25">
      <!--Range:  1 - Inf  Description: Number of festival tickets the winner gets.-->
    </kNumFestivalTicketsForWinning>
    <kNumFestivalTicketsForADraw value="10">
      <!--Range:  1 - Inf  Description: Number of festival tickets both players get in a draw.-->
    </kNumFestivalTicketsForADraw>
    <kNumFestivalTicketsForLosing value="5">
      <!--Range:  1 - Inf  Description: Number of festival tickets the loser gets.-->
    </kNumFestivalTicketsForLosing>
  </Current_Tuning>
</base>