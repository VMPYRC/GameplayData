<?xml version="1.0" encoding="utf-8"?>
<base>
  <Current_Tuning>
    <kStartTime value="8">
      <!--Start time for mail carrier to arrive (in hours)-->
    </kStartTime>
    <kEndTime value="9">
      <!--End time for mail carrier to leave (in hours)-->
    </kEndTime>
    <kDelayBeforeArriving value=".1">
      <!--Length of time (in hours) that deliverer waits before routing to lot-->
    </kDelayBeforeArriving>
    <kDelayBeforeLeaving value=".15">
      <!--Length of time (in hours) that deliverer waits before leaving the lot, after his work is done-->
    </kDelayBeforeLeaving>
    <kExtraWaitTimeAfterSocializing value="0.5">
      <!--Extra time (in hours) to wait before leaving if the service NPC is socialized with-->
    </kExtraWaitTimeAfterSocializing>
    <kServiceTuning>
      <kMaxNumNPCsInPool value="2">
        <!--Max num NPCs at once in service-->
      </kMaxNumNPCsInPool>
      <kCost value="0">
        <!--Cost of service-->
      </kCost>
      <kIsEmergencyService value="False">
        <!--If true this service charges kCost only if falsely called, otherwise free.-->
      </kIsEmergencyService>
      <kIsRecurrent value="True">
        <!--Whether the service is recurrent (true) or one-off (false).  Recurrent services are the only type which can be cancelled through the phone dialog.-->
      </kIsRecurrent>
      <kAlwaysTryToSendSameSim value="True">
        <!--Whether or not the service always tries to send the same NPC to a specific household-->
      </kAlwaysTryToSendSameSim>
    </kServiceTuning>
    <kChanceOfMailmanBeingAngry value="0.2">
      <!--Make the mailman angry. Positive float. Range [0, 1]. Initial GPE default 0.2f-->
    </kChanceOfMailmanBeingAngry>
  </Current_Tuning>
  <Deprecated_Tuning />
</base>
